@page
@model AspNetCore_RazorPages_EFCodeFirst.Pages.AboutModel
@{
    ViewData["Title"] = "学生统计";
}

<h2>学生统计</h2>

<table class="table">
    <tr>
        <th>入学日期</th>
        <th>学生人数</th>
    </tr>
    @foreach (var item in Model.Students)
    {
    <tr>
        <td>@Html.DisplayFor(modelItem=>item.EnrollmentDate)</td>
        <td>@item.StudentCount</td>
    </tr>
    }
</table>